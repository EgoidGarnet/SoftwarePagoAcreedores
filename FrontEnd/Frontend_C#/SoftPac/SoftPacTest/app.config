<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="UsuariosWSPortBinding" />
				<binding name="PaisesWSPortBinding" />
				<binding name="AcreedoresWSPortBinding" />
				<binding name="EntidadesBancariasWSPortBinding" />
				<binding name="MonedasWSPortBinding" />
				<binding name="CuentasPropiasWSPortBinding" />
				<binding name="CuentasAcreedorWSPortBinding" />
				<binding name="FacturasWSPortBinding" />
				<binding name="PropuestaPagoWSPortBinding" />
			</basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/SoftPacWS/UsuariosWS"
                binding="basicHttpBinding" bindingConfiguration="UsuariosWSPortBinding"
                contract="UsuariosWS.UsuariosWS" name="UsuariosWSPort" />
			<endpoint address="http://localhost:8080/SoftPacWS/PaisesWS"
                binding="basicHttpBinding" bindingConfiguration="PaisesWSPortBinding"
                contract="PaisesWS.PaisesWS" name="PaisesWSPort" />
			<endpoint address="http://localhost:8080/SoftPacWS/AcreedoresWS"
                binding="basicHttpBinding" bindingConfiguration="AcreedoresWSPortBinding"
                contract="AcreedoresWS.AcreedoresWS" name="AcreedoresWSPort" />
			<endpoint address="http://localhost:8080/SoftPacWS/EntidadesBancariasWS"
                binding="basicHttpBinding" bindingConfiguration="EntidadesBancariasWSPortBinding"
                contract="EntidadesBancariasWS.EntidadesBancariasWS" name="EntidadesBancariasWSPort" />
			<endpoint address="http://localhost:8080/SoftPacWS/MonedasWS"
                binding="basicHttpBinding" bindingConfiguration="MonedasWSPortBinding"
                contract="MonedasWS.MonedasWS" name="MonedasWSPort" />
			<endpoint address="http://localhost:8080/SoftPacWS/CuentasPropiasWS"
			  binding="basicHttpBinding" bindingConfiguration="CuentasPropiasWSPortBinding"
			  contract="CuentasPropiasWS.CuentasPropiasWS" name="CuentasPropiasWSPort" />
			<endpoint address="http://localhost:8080/SoftPacWS/CuentasAcreedorWS"
			  binding="basicHttpBinding" bindingConfiguration="CuentasAcreedorWSPortBinding"
			  contract="CuentasAcreedorWS.CuentasAcreedorWS" name="CuentasAcreedorWSPort" />
			<endpoint address="http://localhost:8080/SoftPacWS/FacturasWS"
            binding="basicHttpBinding" bindingConfiguration="FacturasWSPortBinding"
            contract="FacturasWS.FacturasWS" name="FacturasWSPort" />
			<endpoint address="http://localhost:8080/SoftPacWS/PropuestaPagoWS"
            binding="basicHttpBinding" bindingConfiguration="PropuestaPagoWSPortBinding"
            contract="PropuestaPagoWS.PropuestaPagoWS" name="PropuestaPagoWSPort" />
        </client>
    </system.serviceModel>
</configuration>